@misc{yang_competitive_2020,
	title = {Competitive Demand Learning: a Data-Driven Pricing Algorithm},
	url = {http://arxiv.org/abs/2008.05195},
	shorttitle = {Competitive Demand Learning},
	abstract = {Dynamic pricing is used to maximize the revenue of a ﬁrm over a ﬁnite-period planning horizon, given that the ﬁrm may not know the underlying demand curve a priori. In emerging markets, in particular, ﬁrms constantly adjust pricing strategies to collect adequate demand information, which is a process known as price experimentation. To date, few papers have investigated the pricing decision process in a competitive environment with unknown demand curves, conditions that make analysis more complex. Asynchronous price updating can render the demand information gathered by price experimentation less informative or inaccurate, as it is nearly impossible for ﬁrms to remain informed about the latest prices set by competitors. Hence, ﬁrms may set prices using available, yet out-of-date, price information of competitors.},
